{
  "JwtSettings": {
    "Issuer": "https://localhost:44351/", //Web Api     
    "Audience": "https://localhost:44358/", //MVC     
    "SecretKey": "any string with 16+ chars", // 16 ou mais caracteres e não deve ficar no appsettings e sim no secrets
    "ExpiryTimeInSeconds": 5000
  },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "UserContext": "Server=(localdb)\\mssqllocaldb;Database=UserContext;Trusted_Connection=True;MultipleActiveResultSets=true"
      "AuthContext": "Server=(localdb)\\mssqllocaldb;Database=AuthContext;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  }
